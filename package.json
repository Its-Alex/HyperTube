{
  "name": "hypertube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start",
    "test-api": "nodemon app/index.js",
    "start-api": "node app/index.js",
    "start": "npm-run-all -p start-js watch-css start-api",
    "build": "npm run build-css && node scripts/build",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "async": "2.5.0",
    "axios": "0.16.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "check-base-encoding": "^1.0.3",
    "cors": "2.8.4",
    "cron": "^1.3.0",
    "exec-sql": "^2.0.1",
    "express": "4.16.2",
    "fluent-ffmpeg": "^2.1.2",
    "langs": "^2.0.0",
    "lodash": "4.17.4",
    "mobx": "3.3.1",
    "mobx-react": "4.3.3",
    "moment": "^2.22.1",
    "mysql": "2.15.0",
    "nodemailer": "^4.4.0",
    "opensubtitles-api": "^4.0.0",
    "passport": "0.4.0",
    "passport-42": "1.2.3",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "pump": "^1.0.3",
    "react": "16.0.0",
    "react-datepicker": "^0.60.2",
    "react-dev-utils": "4.1.0",
    "react-dom": "16.0.0",
    "react-dropzone": "4.2.0",
    "react-infinite-scroller": "^1.1.4",
    "react-moment": "^0.6.5",
    "react-router-dom": "4.2.2",
    "react-transition-group": "2.2.1",
    "semantic-ui-react": "0.75.1",
    "srt2vtt": "^1.3.1",
    "torrent-search": "git+https://github.com/Its-Alex/torrent-search.git",
    "torrent-stream": "^1.0.4",
    "uuid": "3.1.0",
    "validator": "9.0.0",
    "ws": "3.2.0",
    "xtorrent": "0.2.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-app": "3.0.3",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.9.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "node-sass": "4.5.3",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "standard": "10.0.3",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.2",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
